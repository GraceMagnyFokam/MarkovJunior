<!--Introduces the colors the program will use.-->
<sequence values="BUIAREPYNO">
  <prl>
  <!--I (Indigo) is a placeholder color-->
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBIBB/BBBBB" p="0.950006924248719"/>
    <!--The following 8 represent all 8 of the different centroid colors. The p values represent the orientation fraction.-->
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBEBB/BBBBB" p="0.007221"/>
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBABB/BBBBB" p="0.00518329" />
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBUBB/BBBBB" p="0.00399628"/>
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBRBB/BBBBB" p="0.00809148"/>
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBYBB/BBBBB" p="0.00658793"/>
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBNBB/BBBBB" p="0.00676598"/>
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBPBB/BBBBB" p="0.00593507"/>
    <rule in="BBBBB/BBBBB/BBBBB" out="BBBBB/BBOBB/BBBBB" p="0.00728035"/>
  </prl>
  <prl>
  <!--Removes placeholder-->
  <rule in="I" out="B"/>
</prl>
    <all>
    <!--The following shows the growth of each centroid. The p is the volume fraction and represents how large each color centroid should grow.-->
      <rule in="AB" out="*A" p="0.12125349"/>
      <rule in="UB" out="*U" p="0.13650662"/>
      <rule in="RB" out="*R" p="0.10538707"/>
      <rule in="EB" out="*E" p="0.12734683"/>
      <rule in="PB" out="*P" p="0.11907729"/>
      <rule in="YB" out="*Y" p="0.1223218 "/>
      <rule in="NB" out="*N" p="0.13666489"/>
      <rule in="OB" out="*O" p="0.13144202"/>
  </all>
</sequence>
